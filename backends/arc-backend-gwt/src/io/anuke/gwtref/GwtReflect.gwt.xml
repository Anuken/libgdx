                                   value="io.anuke.arc.collection.Queue"/>
    <extend-configuration-property name="gdx.reflect.include"
                                   value="io.anuke.arc.graphics.VertexAttribute"/>
    <extend-configuration-property name="gdx.reflect.include"
                                   value="io.anuke.arc.graphics.g3d.model"/>

